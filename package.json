{
  "name": "react-json",
  "version": "0.0.2",
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint.js --fix src",
    "build:index": "webpack",
    "build:app": "babel --plugins transform-react-jsx src/middle/app/index.jsx -o src/middle/app/index.js",
    "build:comp": "babel --plugins transform-react-jsx src/components -d src/components",
    "build": "npm run build:index && npm run build:app && npm run build:comp",
    "test": "mocha src/**/*.spec.js",
    "start": "node src/server.js"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "immutable": "^3.8.1",
    "koa": "^2.3.0",
    "koa-router": "^7.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "json-server": "^0.12.0",
    "koa-socket.io": "^1.1.2",
    "mocha": "^3.5.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "stryker": "^0.8.0",
    "stryker-api": "^0.7.0",
    "stryker-mocha-runner": "^0.6.0",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
    "webpack": "^3.5.4"
  }
}
