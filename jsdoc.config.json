{
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  
  "recurseDepth": 10,
  
  "source": {
    "include": ["./src", "./webpack", "./webpack.config.js"],
    "exclude": [],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ""
  },
  
  "sourceType": "module",
  
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}